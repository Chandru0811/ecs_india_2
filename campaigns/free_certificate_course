    <script>
      function closeHiringPopup() {
        const errorModal = bootstrap.Modal.getInstance(
          document.getElementById("errorModal")
        );
        const successModal = bootstrap.Modal.getInstance(
          document.getElementById("successModal")
        );

        if (errorModal) {
          errorModal.hide();
        }
        if (successModal) {
          successModal.hide();
        }
      }
    </script>
    <script>
      const roles = document.querySelectorAll(".role");
      const image = document.getElementById("dynamic-image");
      let currentIndex = 0;

      // Array of image sources corresponding to roles
      const images = [
        "../assets/images/free_certificate_content_1.webp",
        "../assets/images/free_certificate_content_2.webp",
        "../assets/images/free_certificate_content_4.webp",
        "../assets/images/free_certificate_content_5.webp",
        "../assets/images/free_certificate_content_3.webp",
      ];

      function updateActiveRole(index) {
        roles.forEach((role, i) => {
          role.classList.toggle("active", i === index);
        });
        // Change the image based on the active role
        image.src = images[index];
      }

      // Set up interval for auto-change
      setInterval(() => {
        currentIndex = (currentIndex + 1) % roles.length;
        updateActiveRole(currentIndex);
      }, 9000);

      // Add click event listener
      roles.forEach((role, index) => {
        role.addEventListener("click", () => {
          currentIndex = index;
          updateActiveRole(index);
        });
      });

      // Initialize the first active role and image
      updateActiveRole(currentIndex);
    </script>
  </body>
</html>
