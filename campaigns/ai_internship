    <script>
      function closeHiringPopup() {
        const errorModal = bootstrap.Modal.getInstance(
          document.getElementById("errorModal")
        );
        const successModal = bootstrap.Modal.getInstance(
          document.getElementById("successModal")
        );

        if (errorModal) {
          errorModal.hide();
        }
        if (successModal) {
          successModal.hide();
        }
      }
    </script>
